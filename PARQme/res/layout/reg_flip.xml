<?xml version="1.0" encoding="utf-8"?>
<ViewFlipper xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent" android:id="@+id/reg_flip">

	<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:orientation="vertical" android:layout_width="match_parent"
		android:layout_height="match_parent">
		<EditText android:id="@+id/emailForm" android:text="Email and Password"
			android:inputType="textEmailAddress" android:layout_width="match_parent"
			android:layout_height="wrap_content" android:layout_alignParentTop="true"
			android:layout_alignParentLeft="true" android:layout_marginTop="132dp"></EditText>
		<EditText android:id="@+id/passwordForm" android:inputType="textPassword"
			android:layout_width="match_parent" android:layout_height="wrap_content"
			android:layout_below="@+id/emailForm" android:layout_alignParentLeft="true"></EditText>
		<Button android:background="@drawable/login" android:layout_width="match_parent"
			android:id="@+id/loginButton" android:layout_height="wrap_content"
			android:layout_below="@+id/passwordForm"
			android:layout_alignParentLeft="true" android:layout_marginTop="51dp"></Button>
		<Button android:background="@drawable/register" android:layout_width="match_parent"
			android:id="@+id/registerbutton" android:layout_height="wrap_content"
			android:layout_below="@+id/loginButton"
			android:layout_alignParentLeft="true"></Button>
		<CheckBox android:id="@+id/checkBox1" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Remember Me"
			android:layout_below="@+id/passwordForm"
			android:layout_alignParentLeft="true" android:layout_marginLeft="72dp"></CheckBox>

	</RelativeLayout>

	<RelativeLayout android:id="@+id/LinearLayout1"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent">
	<Button android:background="@drawable/logout"
		android:layout_width="match_parent" android:id="@+id/logoutbutton"
		android:layout_height="wrap_content" android:layout_alignParentBottom="true"
		android:layout_alignParentLeft="true"></Button>
	<Button android:layout_height="wrap_content" android:id="@+id/setting"
		android:background="@drawable/setting" android:layout_width="wrap_content"
		android:layout_above="@+id/logoutbutton" android:layout_alignLeft="@+id/logoutbutton"
		android:layout_alignRight="@+id/logoutbutton"></Button>
	<TextView android:textAppearance="?android:attr/textAppearanceLarge"
		android:layout_width="wrap_content" android:text="Account Information"
		android:layout_height="wrap_content" android:id="@+id/accinfolabel"
		android:layout_alignParentTop="true" android:layout_centerHorizontal="true"
		android:layout_marginTop="97dp"></TextView>
	<TextView android:textAppearance="?android:attr/textAppearanceLarge"
		android:layout_width="wrap_content" android:text="Email: "
		android:layout_height="wrap_content" android:id="@+id/accemailfield"
		android:layout_below="@+id/accinfolabel"
		android:layout_alignParentLeft="true"></TextView>
	<TextView android:textAppearance="?android:attr/textAppearanceMedium"
		android:layout_width="wrap_content" android:text="City: "
		android:layout_height="wrap_content" android:id="@+id/acccityfield"
		android:layout_below="@+id/accemailfield"
		android:layout_alignParentLeft="true"></TextView>
	<TextView android:textAppearance="?android:attr/textAppearanceMedium"
		android:layout_width="wrap_content" android:text="CC: "
		android:layout_height="wrap_content" android:id="@+id/creditcardfield"
		android:layout_below="@+id/acccityfield"
		android:layout_alignParentLeft="true"></TextView>

</RelativeLayout>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent">
	<Button android:layout_width="wrap_content" android:text="Button"
		android:id="@+id/settingback" android:layout_height="wrap_content"
		android:layout_alignParentBottom="true"
		android:layout_alignParentLeft="true"></Button>
	<CheckBox android:id="@+id/checkBox1" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="CheckBox"
		android:layout_alignParentTop="true" android:layout_alignParentLeft="true"
		android:layout_marginTop="116dp"></CheckBox>
	<CheckBox android:id="@+id/checkBox2" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="CheckBox"
		android:layout_below="@+id/checkBox1" android:layout_alignParentLeft="true"></CheckBox>
	<CheckBox android:id="@+id/checkBox3" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="CheckBox"
		android:layout_below="@+id/checkBox2" android:layout_alignParentLeft="true"></CheckBox>
	<CheckBox android:id="@+id/checkBox4" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="CheckBox"
		android:layout_below="@+id/checkBox3" android:layout_alignParentLeft="true"></CheckBox>

</RelativeLayout>
</ViewFlipper>
